        <!--0:Don’t Show-->
        <!--1:Front Wheel Drive-->
        <!--2:Rear Wheel Drive-->
        <!--3:Front And Rear Whell Drive-->
        <TireToBattery type="int">False</TireToBattery>
    </EnergyFlowLineIndication>
    <EnergyFlow>
        <!--Precision:Not Applicable-->
        <!--false:Don’t Show-->
        <!--true:Show-->
        <AGCVisible type="bool">false</AGCVisible>
