<EnergyMonitor name="EnergyMonitor">
    <!--Precision:Not Applicable-->
    <!--false:Don’t Show-->
    <!--true:Show-->
    <BatteryIcon type="bool">false</BatteryIcon>
    <!--Precision:Not Applicable-->
    <!--0:Don,t Show-->
    <!--1:PHV Mode-->
    <!--2:HV Mode-->
    <!--3:EV Mode-->
    <!--Other values need to be defined-->
    <EnergyFlowOveralControl type="int">0</EnergyFlowOveralControl>
    <!--Precision:Not Applicable-->
    <!--0:null-->
    <!--|-->
    <!--8:full-->
    <BatteryLevel type="int">0</BatteryLevel>
    <EnergyFlowLineIndication>
        <!--Precision:Not Applicable-->
        <!--0:Don’t Show-->
        <!--1:Front Wheel Drive-->
        <!--2:Rear Wheel Drive-->
        <!--3:Front And Rear Whell Drive-->
        <EngineToTire type="int">0</EngineToTire>
        <!--Precision:Not Applicable-->
        <!--false:Don’t Show-->
        <!--true:Show-->
        <EngineToBattery type="bool">false</EngineToBattery>
        <!--Precision:Not Applicable-->
        <!--0:Don’t Show-->
        <!--1:Front Wheel Drive-->
        <!--2:Rear Wheel Drive-->
        <!--3:Front And Rear Whell Drive-->
        <BatteryToTire type="int">False</BatteryToTire>
