<WheelWriveandGravity name="WheelWriveandGravity">
    <!--0:Without center differential lock-->
    <!--1:With center differential lock and under center differential unlock condition-->
    <!--2:With center differential lock and under center differential lock condition-->
    <CenterDifferentialLock type="int">0</CenterDifferentialLock>
    <!--0:2WD-->
    <!--1:4WD-->
    <!--2:AWD-->
    <State type="int">0</State>
    <Shaft>
        <!--false:No drive force transmission-->
        <!--true:Drive force transmission-->
        <FL type="bool">False</FL>
        <!--false:No drive force transmission-->
        <!--true:Drive force transmission-->
        <FR type="bool">False</FR>
        <!--false:No drive force transmission-->
        <!--true:Drive force transmission-->
        <RL type="bool">False</RL>
        <!--false:No drive force transmission-->
        <!--true:Drive force transmission-->
        <RR type="bool">False</RR>
        <!--0:Without front propeller shaft-->
        <!--1:With front propeller shaft and no drive force transmission to front propeller shaft-->
        <!--2:With front propeller shaft and drive force transmission to front propeller shaft-->
        <FrontPropeller type="int">0</FrontPropeller>
        <!--0:Without rear propeller shaft-->
        <!--1:With rear propeller shaft and no drive force transmission to rear propeller shaft-->
        <!--2:With rear propeller shaft and drive force transmission to rear propeller shaft-->
        <RearPropeller type="int">0</RearPropeller>
    </Shaft>
    <Axle>
        <!--false:No drive force transmission to front axle-->
        <!--true:Drive force transmission to front axle-->
        <Front type="bool">False</Front>
        <!--0:Rr differential unit is displayed and drive force transmission to rear differential-->
        <!--1:R-Four unit is displayed and no drive force transmission to E-Four unit-->
        <!--2:R-Four unit is displayed and drive force transmission to E-Four unit-->
        <Rear type="int">0</Rear>
    </Axle>
    <WheelDriveForce>
        <!--0:No drive force transmission-->
        <!--1:amount of 1 segment -->
        <!--2:amount of 2 segment -->
        <!--3:amount of 3 segment -->
        <!--4:amount of 4 segment -->
        <!--5:amount of 5 segment -->
        <FL type="int">0</FL>
        <!--0:No drive force transmission-->
        <!--1:amount of 1 segment -->
        <!--2:amount of 2 segment -->
        <!--3:amount of 3 segment -->
        <!--4:amount of 4 segment -->
        <!--5:amount of 5 segment -->
        <FR type="int">0</FR>
        <!--0:No drive force transmission-->
        <!--1:amount of 1 segment -->
        <!--2:amount of 2 segment -->
        <!--3:amount of 3 segment -->
        <!--4:amount of 4 segment -->
        <!--5:amount of 5 segment -->
        <RL type="int">0</RL>
        <!--0:No drive force transmission-->
        <!--1:amount of 1 segment -->
        <!--2:amount of 2 segment -->
        <!--3:amount of 3 segment -->
        <!--4:amount of 4 segment -->
        <!--5:amount of 5 segment -->
        <RR type="int">0</RR>
    </WheelDriveForce>
</WheelWriveandGravity>
